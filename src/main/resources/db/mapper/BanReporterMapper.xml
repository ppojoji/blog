<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="BanReporterMapper">
	<insert id="insertBan" parameterType="map">
	 	INSERT INTO tb_ban_report(reporter,target,targettype,bancode) 
	 	values(#{user.seq},#{replySeq},#{targetType},#{banCode}) 
	 </insert>
	 <select id="banList" resultType="map">
	 	select * 
	 	from tb_ban_report
	 </select>
  </mapper>
